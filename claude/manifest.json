{
  "manifest_version": 3,
  "name": "Smart Form Filler",
  "version": "1.0",
  "description": "Automatically fill forms using predefined data and AI assistance",
  "permissions": ["activeTab", "storage", "scripting", "contextMenus"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Smart Form Filler"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon16.png",
    "128": "icons/icon16.png"
  }
}
